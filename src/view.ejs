<head>
  <meta charset="UTF-8">
  <title>PAGE TITLE</title>
  <% include view/_header %>
</head>
<body>
  <div class="l-wrap">
    <%- include('./view/layout/_l-header'); %>
    <main class="l-main">
      <div class="hero-panorama">
        <div class="hero-panorama-header">
          <div class="l-container">
            <ul>
              <li><a href="#js-floor-8" class="is-active js-floor-link">8th Floor</a></li>
              <li><a href="#js-floor-33" class="js-floor-link">33rd Floor</a></li>
              <li><a href="#js-floor-43" class="js-floor-link">43rd Floor</a></li>
            </ul>
          </div>
        </div>
        <div class="hero-panorama-list">
          <div class="hero-panorama-item js-floor" id="js-floor-8">
            <div class="cyclorama">
                <div class="pan-inner">
                    <div class="pan-content">
                        <!-- put 360° panorama image(s) here -->
                        <img src="http://www.keauhouplace.com/assets/img/views/view_08_Fl.jpg">
                    </div>
                </div>
                <button class="pan-left">
                  <span></span>
                </button>
                <button class="pan-right">
                  <span></span>
                </button>
                <div class="pan-percentage">
                    <div class="pan-runner"></div>
                </div>
            </div>
          </div>
          <div class="hero-panorama-item js-floor" id="js-floor-33">
            <div class="cyclorama">
                <div class="pan-inner">
                    <div class="pan-content">
                        <!-- put 360° panorama image(s) here -->
                        <img src="http://www.keauhouplace.com/assets/img/views/view_33_Fl.jpg">
                    </div>
                </div>
                <button class="pan-left">
                  <span></span>
                </button>
                <button class="pan-right">
                  <span></span>
                </button>
                <div class="pan-percentage">
                    <div class="pan-runner"></div>
                </div>
            </div>
          </div>
          <div class="hero-panorama-item js-floor" id="js-floor-43">
            <div class="cyclorama">
                <div class="pan-inner">
                    <div class="pan-content">
                        <!-- put 360° panorama image(s) here -->
                        <img src="http://www.keauhouplace.com/assets/img/views/view_43_Fl.jpg">
                    </div>
                </div>
                <button class="pan-left">
                  <span></span>
                </button>
                <button class="pan-right">
                  <span></span>
                </button>
                <div class="pan-percentage">
                    <div class="pan-runner"></div>
                </div>
            </div>
          </div>
        </div>
      </div>
    </main>
    <%- include('./view/layout/_l-footer'); %>
  </div>

<script src="assets/js/vendor.js"></script>
<script src="assets/js/app.js"></script>
<script src="assets/lib/jquery.cyclorama.js"></script>
<script src="assets/js/view.js"></script>
</body>
</html>